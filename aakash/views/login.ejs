<% include partials/header %>

  <div class="limiter">
    <div class="container-login100">
      <div class="wrap-login100 p-t-50 p-b-90">
        <form class="login100-form flex-sb flex-w" action="/booktique/login" method="POST">
          <span class="login100-form-title p-b-51">Login To Booktique</span>

          <div class="wrap-input100 m-b-16" data-validate="Username is required">
            <input class="input100" type="text" name="username" placeholder="Username">
          </div>

          <div class="wrap-input100 m-b-16" data-validate="Password is required">
            <input class="input100" type="password" name="password" placeholder="Password">
          </div>

          <div class="container-login100-form-btn m-t-17">
            <button class="login100-form-btn">Login</button>
          </div>

        </form>
      </div>
    </div>
  </div>

<% include partials/footer %>
